<?xml version="1.0" encoding="utf-8"?>
<resources>
    <item name="launch_screen" type="layout">
        @layout/launch_screen
    </item>
    <string name="app_name">LaunchScreen</string>
    <style name="LaunchScreen_Fullscreen" parent="LaunchScreen_LaunchTheme">
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="LaunchScreen_LaunchAnimation">
        <item name="android:windowExitAnimation">@android:anim/fade_out</item>
    </style>
    <style name="LaunchScreen_LaunchTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowAnimationStyle">@style/LaunchScreen_LaunchAnimation</item>
    </style>
</resources>